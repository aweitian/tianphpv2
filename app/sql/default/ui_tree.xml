<?xml version="1.0" encoding="UTF-8"?>
<ui_tree>
	<all>
		SELECT 
			`sid`,
			`pid`,
			`name`,
			`url`,
			`order`,
			`layout` FROM `data_tree` WHERE 1
	</all>

	
	<!-- 返回字段有:trd -->
	<getTrdByAid>
		SELECT
			  `trd`
			FROM `data_article_tree`
			WHERE `data_article_tree`.`aid` = :aid
	</getTrdByAid>
</ui_tree>