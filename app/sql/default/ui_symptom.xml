<?xml version="1.0" encoding="UTF-8"?>
<ui_symptom>
	<all>
	 	SELECT 
	 		`data_attr_data`.`sid`,
	 		`data_attr_data`.`key`,
	 		`data_attr_data`.`data`,
	 		`data_attr_data`.`pid`,
	 		`data_attr_data`.`metaid`
	 		FROM `data_attr_data`
			WHERE `grp` = 2;		
	</all>
