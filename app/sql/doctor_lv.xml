			INSERT INTO `data_doctor_ext` (
				`dod`,`dlv`
			) VALUES (
				:dod,:dlv
			)
		</add>
		<update>
			UPDATE `data_doctor_ext` SET 
			`dlv`=:dlv
		 WHERE `dod`=:dod
		</update>
	
	</lv>
	
	
</sql>