<sql>
	<all>
		SELECT * FROM `data_doctor_lv_meta` WHERE 1
	</all>

	<add>
		INSERT INTO `data_doctor_lv_meta`(`data`) VALUES (:data)
	</add>
	
	<update>
		UPDATE `data_doctor_lv_meta` SET `data`=:data WHERE `sid` = :sid
	</update>
	
	<remove>
		DELETE FROM `data_doctor_lv_meta` WHERE `sid` = :sid
	</remove>
	
	<lv>
